<gsa-template>
    <header>
        <name>Training Repository</name>
        <author>Ramesh Sankaran</author>
    </header>
    <!--Item Descriptor user for Citrisys ATG Training.-->
    <item-descriptor name="user">
        <table name="training_user" type="primary" id-column-names="id">
            <property name="firstName" data-type="string"/>
            <property name="lastName" data-type="string"/>
            <property name="email" data-type="string"/>
            <property name="gender" data-type="string"/>
        </table>
        <!--Multi table to persist one-to-many relationship.-->
        <table name="training_user_addresses" type="multi" id-column-names="training_id">
            <property name="address_multi" column-names="id" data-type="" component-item-type="address"/>
        </table>
    </item-descriptor>
    <!--Item Descriptor address for Citrisys ATG training.-->
    <item-descriptor name="address">
        <table name="training_address" type="primary" id-column-names="id">
            <property name="address1" data-type="string"/>
            <property name="city" data-type="string"/>
            <property name="state" data-type="string"/>
            <property name="country" data-type="string"/>
            <property name="postalCode" data-type="string"/>
            <!--Property to persist one-to-one relation with user item descriptor.-->
            <property name="user" column-names="user" item-type="user" repository=""/>
        </table>
    </item-descriptor>
</gsa-template>
